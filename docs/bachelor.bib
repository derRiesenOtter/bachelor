	keywords = {Proteins, Organelles, protein phase separation, Amino Acid Substitution, Biomolecular Condensates, Cytoplasm, intrinsically disordered proteins, prediction methods},
	file = {Full Text:/Users/robin/Zotero/storage/RKRFJSW7/Kuechler et al. - 2023 - Comparison of Biomolecular Condensate Localization and Protein Phase Separation Predictors.pdf:application/pdf},
